<form
	action="./api/v1/productos"
	method="post"
	class="row container mx-auto d-flex justify-content-center"
>
	<div class="mb-5 col-9">
		<label
			for="titleProd"
			class="form-label"
			>Nombre del producto</label
		>
		<input
			type="text"
			class="form-control"
			id="titleProd"
			name="title"
			required
		/>
	</div>
	<div class="mb-5 col-9">
		<label
			for="priceProd"
			class="form-label"
			>Precio $</label
		>
		<input
			type="number"
			class="form-control"
			id="priceProd"
			name="price"
			required
		/>
	</div>
	<div class="mb-5 col-9">
		<label
			for="thumbnailProd"
			class="form-label"
			>Imagen del producto</label
		>
		<input
			type="text"
			class="form-control"
			id="thumbnailProd"
			name="thumbnail"
			required
		/>
	</div>
	<div class="d-flex justify-content-center">
		<button
			type="submit"
			class="btn btn-dark mb-4 px-5"
		>
			Agregar
		</button>
	</div>
</form>
