<h2
	id="productList"
	class="text-center fs-2 fw-bolder my-5"
>
	Lista de productos
</h2>
<table class="table table-striped table-hover">
	<thead>
		<tr>
			<th scope="col"># Id</th>
			<th scope="col">TÃ­tulo</th>
			<th scope="col">Precio</th>
			<th scope="col">Imagen</th>
		</tr>
	</thead>
	<tbody id="trTable">
		<% products.forEach(prod => { %>
		<tr>
			<td><%= prod.id %></td>
			<td><%= prod.title %></td>
			<td>$ <%= prod.price %></td>
			<td>
				<img
					src="<%= prod.url %>"
					alt="<%= prod.title %>"
					width="55"
				/>
			</td>
		</tr>
		<% }) %>
	</tbody>
</table>
